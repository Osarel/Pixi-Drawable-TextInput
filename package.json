    "TypeScript",
    "Input",
    "TextInput",
    "Text"
  ],
  "bugs": {
    "url": "https://github.com/Osarel/Pixi-Drawable-TextInput/issues"
  },
  "homepage": "https://osarel.github.io/Pixi-Drawable-TextInput/",
  "devDependencies": {
    "parcel-bundler": "^1.12.4",
